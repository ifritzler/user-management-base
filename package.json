{
    "name": "user-management",
    "version": "1.0.0",
    "type": "module",
    "description": "Aplicacion para la gestion de usuarios de futuros proyectos web",
    "scripts": {
        "dev": "nodemon src/index.js",
        "start": "node src/index.js",
        "lint": "eslint src/**/*.js --fix",
        "prettier": "prettier --write .",
        "prepare": "husky install",
        "postinstall": "npm run prepare"
    },
    "author": {
        "name": "Fritzler Ilan E.",
        "url": "https://github.com/ifritzler"
    },
    "license": "MIT",
    "dependencies": {
        "@sinclair/typebox": "0.24.13",
        "ajv": "8.11.0",
        "ajv-errors": "3.0.0",
        "ajv-formats": "2.1.1",
        "dotenv": "16.0.1",
        "express": "4.18.1",
        "jose": "4.8.3",
        "mongoose": "6.4.4"
    },
    "devDependencies": {
        "eslint": "8.18.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-config-standard": "17.0.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-n": "15.2.3",
        "eslint-plugin-promise": "6.0.0",
        "husky": "8.0.1",
        "lint-staged": "13.0.2",
        "nodemon": "2.0.19",
        "prettier": "2.7.1"
    },
    "imports": {
        "#Lib/*": "./src/lib/*",
        "#Config/*": "./src/config/*",
        "#Controllers/*": "./src/controllers/*",
        "#Validations/*": "./src/validations/*",
        "#Schemas/*": "./src/schemas/*",
        "#Routes/*": "./src/routes/*"
    },
    "lint-staged": {
        "*.js": "eslint --fix",
        "*.{js,md}": "prettier --write"
    }
}
